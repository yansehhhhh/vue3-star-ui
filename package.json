{
    "name": "vue3-star-ui",
    "private": false,
    "version": "0.0.4",
    "description": "vue3的pc端ui库",
    "main": "dist/lib.umd.js",
    "module": "dist/lib.es.js",
    "style": "dist/style.css",
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/lib.es.js",
            "require": "./dist/lib.umd.js"
        },
        "./*": "./*"
    },
    "scripts": {
        "dev": "vite --mode dev --host 0.0.0.0",
        "build": "vite build --mode production",
        "preview": "vite preview"
    },
    "keywords": [
        "vue3",
        "vite",
        "component",
        "ui",
        "javascript"
    ],
    "engines": {
        "node": ">= 16"
    },
    "dependencies": {
        "vue": "^3.2.37",
        "vue-router": "^4.0.13"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^3.1.0",
        "less": "^4.1.3",
        "vite": "^3.1.0"
    }
}